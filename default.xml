<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="neapp"
           fetch=".." />

  <default revision="v1.7"
           remote="neapp"
           sync-j="4" />

  <project path="Browsers/neon-extension-browser-base" name="neon-extension-browser-base" groups="chrome,firefox" />
  <project path="Browsers/neon-extension-browser-chrome" name="neon-extension-browser-chrome" groups="chrome" />
  <project path="Browsers/neon-extension-browser-firefox" name="neon-extension-browser-firefox" groups="firefox" />
  <project path="Browsers/neon-extension-browser-webextension" name="neon-extension-browser-webextension" groups="chrome,firefox" />

  <project path="Destinations/neon-extension-destination-lastfm" name="neon-extension-destination-lastfm" groups="chrome,firefox" />
  <project path="Destinations/neon-extension-destination-librefm" name="neon-extension-destination-librefm" groups="chrome,firefox" />
  <project path="Destinations/neon-extension-destination-listenbrainz" name="neon-extension-destination-listenbrainz" groups="chrome,firefox" />

  <project path="Packages/neon-extension-chrome" name="neon-extension-chrome" groups="chrome" />
  <project path="Packages/neon-extension-firefox" name="neon-extension-firefox" groups="firefox" />

  <project path="Sources/neon-extension-source-googlemusic" name="neon-extension-source-googlemusic" groups="chrome,firefox" />

  <project path="Tools/neon-extension-build" name="neon-extension-build" groups="chrome,firefox">
    <linkfile src="scripts/build.cmd" dest="build.cmd"/>
    <linkfile src="scripts/setup.cmd" dest="setup.cmd"/>
  </project>

  <project path="neon-extension-framework" name="neon-extension-framework" groups="chrome,firefox" />
  <project path="neon-extension-core" name="neon-extension-core" groups="chrome,firefox" />
</manifest>
